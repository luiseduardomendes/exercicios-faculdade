    "import sympy as sp\n",
    "import numpy as np\n",
    "sp.init_printing()\n",
    "\n",
    "\n",
    "velocity = np.array([395.4, 98.7, 0]) * 10**3\n",
    "magField = np.array([0.03, 0.09, 0])\n",
    "\n",
    "crossProduct = np.cross(velocity, magField)\n",
    "\n",
    "charge = 319.1 * 10 **(-6)\n",
    "\n",
    "np.linalg.norm(crossProduct*charge)\n"
   ]
