   "source": [
    "import sympy as sp\n",
    "sp.init_printing()\n",
    "\n",
    "x, y = sp.symbols(['x', 'y'])\n",
    "\n",
    "f_xy = y**2 * x\n",
    "\n",
    "# volume by double integration\n",
    "sp.integrate(f_xy, (y, -x, x**2), (x, 0, 1))"
   ]
  }
